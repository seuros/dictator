{
  "release-type": "rust",
  "plugins": [
    {
      "type": "cargo-workspace"
    },
    {
      "type": "linked-versions",
      "groupName": "dictator",
      "components": [
        "dictator",
        "dictator-core",
        "dictator-decree-abi",
        "dictator-supreme",
        "dictator-ruby",
        "dictator-typescript",
        "dictator-golang",
        "dictator-rust",
        "dictator-python",
        "dictator-frontmatter"
      ],
      "merge": false
    }
  ],
  "packages": {
    "crates/dictator": {
      "component": "dictator"
    },
    "crates/dictator-core": {
      "component": "dictator-core"
    },
    "crates/dictator-decree-abi": {
      "component": "dictator-decree-abi"
    },
    "crates/dictator-supreme": {
      "component": "dictator-supreme"
    },
    "crates/dictator-ruby": {
      "component": "dictator-ruby"
    },
    "crates/dictator-typescript": {
      "component": "dictator-typescript"
    },
    "crates/dictator-golang": {
      "component": "dictator-golang"
    },
    "crates/dictator-rust": {
      "component": "dictator-rust"
    },
    "crates/dictator-python": {
      "component": "dictator-python"
    },
    "crates/dictator-frontmatter": {
      "component": "dictator-frontmatter"
    }
  }
}
